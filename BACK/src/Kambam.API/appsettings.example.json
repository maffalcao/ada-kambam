{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Postgres": "Host=host; Database=dataBase; Username=userName; Password=password"
  },
  "Jwt": {
    "Key": "jwtKey",
    "Issuer": "jwtIssuer",
    "Audience": "JwtAudience",
    "ExpirationMinutes": 30
  },
  "LoginAuth": {
    "User": "userName",
    "Password": "userPassword"
  }
}